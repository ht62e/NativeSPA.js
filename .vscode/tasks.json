{
    "version": "2.0.0",
    "command": "tsc",
    "tasks": [
        {
            "label": "debug-build",
            "type": "typescript",
            "tsconfig": "tsconfig.json",
            "problemMatcher": [
                "$tsc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "release-build",
            "type": "typescript",
            "tsconfig": "tsconfig.release.json",
            "problemMatcher": [
                "$tsc"
            ],
            "group": "build"
        }
    ]
}